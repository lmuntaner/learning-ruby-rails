<h1>Sign up</h1>

<form action="<%= users_url %>" method="post">
  <%= render 'layouts/auth_input' %>
  
  <label>
    Username
    <input type="text" name="user[username]">
  </label>
  
  <label>
    Password
    <input type="password" name="user[password]">
  </label>
  
  <input type="submit" value="Sign up">
</form>